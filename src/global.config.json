{
  "slides": {
    "progress": {
      "offsetSide": 5,
      "offset": 10,
      "length": 3,
      "thickness": 1
    },
    "control": {
      "length": 2.4,
      "thickness": 1.5,
      "opacityOnTouch": 0.3,
      "customWheelThreshold": 60,
      "customScrollThreshold": {
        "y": 12.5
      },
      "customSwipeThreshold": {
        "x": 12.5,
        "y": 5
      }
    },
    "animation": {
      "duration": 1.8,
      "experienceLine": {
        "duration": 1.2,
        "ease": "power3.out"
      },
      "offerButtonDelay": 1000
    },
    "style": {
      "backgroundOpacity": 0.4
    }
  },
  "parallax": {
    "animation": {
      "textIn": {
        "duration": 1.2,
        "ease": "power2.out"
      },
      "objects": {
        "duration": 1.4,
        "delay": 0.1,
        "ease": "power2.out"
      }
    },
    "userInteraction": {
      "hoveringTouch": 1.5,
      "hoveringMouse": 3,
      "swipeDelay": 12
    }
  }
}
