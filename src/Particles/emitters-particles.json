{
  "autoPlay": true,
  "fullScreen": {
    "enable": true
  },
  "detectRetina": true,
  "fpsLimit": 144,
  "particles": {
    "color": {
      "value": ["#ff1ce7", "#1cddff"]
    },
    "move": {
      "outModes": {
        "default": "destroy"
      },
      "distance": {
        "horizontal": 75,
        "vertical": 75
      },
      "direction": "none",
      "enable": true,
      "random": true,
      "speed": {
        "min": 0.2,
        "max": 0.8
      }
    },
    "number": {
      "value": 0
    },
    "opacity": {
      "value": {
        "min": 0.4,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 150,
        "decay": 1,
        "delay": 0,
        "sync": true,
        "mode": "random",
        "startValue": "random",
        "destroy": "none"
      }
    },
    "shape": {
      "close": true,
      "fill": true,
      "type": "triangle"
    },
    "size": {
      "value": {
        "min": 0.1,
        "max": 2.2
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 13,
        "sync": false,
        "mode": "random",
        "startValue": "max",
        "destroy": "none"
      }
    },
    "stroke": {
      "width": 0.1
    },
    "links": {
      "color": "random",
      "distance": 8,
      "enable": true,
      "frequency": 0.15,
      "opacity": 1,
      "triangles": {
        "enable": true,
        "frequency": 1
      },
      "width": 0.75
    }
  },
  "pauseOnBlur": true,
  "pauseOnOutsideViewport": true,
  "name": "Emitters",
  "backgroundMode": {
    "enable": true,
    "zIndex": 0
  },
  "interactivity": {
    "detectsOn": "window",
    "events": {
      "onHover": {
        "enable": true,
        "mode": ["attract"],
        "parallax": {
          "enable": false,
          "force": 120,
          "smooth": 5
        }
      }
    },
    "modes": {
      "trail": {
        "delay": 0,
        "pauseOnStop": true,
        "quantity": 1
      },
      "attract": {
        "distance": 100,
        "duration": 0,
        "easing": "ease-out-quad",
        "factor": 0.07,
        "maxSpeed": 50,
        "speed": 30
      },
      "slow": {
        "factor": 1,
        "radius": 60
      }
    }
  },
  "emitters": []
}
