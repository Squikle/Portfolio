{
  "autoPlay": false,
  "fullScreen": {
    "enable": false
  },
  "detectRetina": true,
  "duration": 0,
  "fpsLimit": 30,
  "particles": {
    "rotate": {
      "value": {
        "min": 0,
        "max": 360
      },
      "animation": {
        "enable": true,
        "speed": 60,
        "decay": 0,
        "sync": false
      },
      "direction": "random",
      "path": false
    },
    "color": {
      "value": ["#ff1ce7", "#1cddff"]
    },
    "move": {
      "distance": {
        "horizontal": 25,
        "vertical": 25
      },
      "enable": true,
      "random": true,
      "size": false,
      "speed": {
        "min": 1,
        "max": 1
      },
      "warp": true
    },
    "number": {
      "density": {
        "enable": false,
        "height": 900
      },
      "limit": {
        "mode": "delete",
        "value": 0
      },
      "value": 0.8
    },
    "opacity": {
      "value": {
        "min": 0.6,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 150,
        "decay": 1,
        "delay": 0,
        "sync": true,
        "mode": "random",
        "startValue": "random",
        "destroy": "none"
      }
    },
    "shape": {
      "close": true,
      "fill": true,
      "options": {},
      "type": "triangle"
    },
    "size": {
      "value": {
        "min": 1,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 13,
        "decay": 0,
        "delay": 0,
        "sync": false,
        "mode": "random",
        "startValue": "max",
        "destroy": "none"
      }
    },
    "stroke": {
      "width": 1
    },
    "links": {
      "blink": false,
      "color": "random",
      "consent": false,
      "distance": 1,
      "enable": true,
      "frequency": 0.15,
      "opacity": 1,
      "shadow": {
        "blur": 1,
        "color": {
          "value": "#ff9900"
        },
        "enable": false
      },
      "triangles": {
        "enable": true,
        "frequency": 1
      },
      "width": 1,
      "warp": false
    }
  },
  "zLayers": 100,
  "name": "Canvas",
  "canvasMask": {
    "enable": true,
    "image": {
      "src": "/squik-small.png"
    },
    "position": {
      "x": 50,
      "y": 50
    },
    "override": {
      "color": true,
      "opacity": true
    },
    "scale": 3.5
  },
  "backgroundMode": {
    "enable": true,
    "zIndex": 0
  },
  "interactivity": {
    "detectsOn": "window",
    "events": {
      "onHover": {
        "enable": true,
        "mode": ["bubble", "attract"],
        "parallax": {
          "enable": true,
          "force": 150,
          "smooth": 6
        }
      }
    },
    "modes": {
      "attract": {
        "distance": 0.7,
        "duration": 0,
        "easing": "ease-out-quad",
        "factor": 0.05,
        "maxSpeed": 70,
        "speed": 1
      },
      "bubble": {
        "distance": 1,
        "duration": 0,
        "mix": true,
        "opacity": 1,
        "size": 1.5
      }
    }
  }
}
