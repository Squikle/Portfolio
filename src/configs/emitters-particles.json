{
  "autoPlay": false,
  "fullScreen": {
    "enable": false
  },
  "detectRetina": true,
  "fpsLimit": 120,
  "particles": {
    "color": {
      "value": [
        "#ff1ce7",
        "#1cddff"
      ]
    },
    "move": {
      "outModes": {
        "default": "destroy"
      },
      "distance": {
        "horizontal": 75,
        "vertical": 75
      },
      "direction": "none",
      "enable": true,
      "random": true,
      "speed": {
        "min": 1,
        "max": 1
      }
    },
    "number": {
      "value": 0
    },
    "opacity": {
      "value": {
        "min": 0.4,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 150,
        "decay": 1,
        "delay": 0,
        "sync": true,
        "mode": "random",
        "startValue": "random",
        "destroy": "none"
      }
    },
    "shape": {
      "close": true,
      "fill": true,
      "type": "triangle"
    },
    "size": {
      "value": {
        "min": 1,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 13,
        "sync": false,
        "mode": "random",
        "startValue": "max",
        "destroy": "none"
      }
    },
    "stroke": {
      "width": 1
    },
    "links": {
      "color": "random",
      "distance": 1,
      "enable": true,
      "frequency": 0.25,
      "opacity": 1,
      "triangles": {
        "enable": true,
        "frequency": 0.7
      },
      "width": 1
    }
  },
  "name": "Emitters",
  "backgroundMode": {
    "enable": true,
    "zIndex": 0
  },
  "interactivity": {
    "detectsOn": "window",
    "events": {
      "onHover": {
        "enable": true,
        "mode": [
          "attract"
        ]
      }
    },
    "modes": {
      "attract": {
        "distance": 1,
        "duration": 0,
        "easing": "ease-out-quad",
        "factor": 0.07,
        "maxSpeed": 50,
        "speed": 1
      }
    }
  },
  "emitters": []
}
