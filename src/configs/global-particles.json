{
  "autoPlay": false,
  "delay": 0,
  "fullScreen": {
    "enable": false
  },
  "detectRetina": true,
  "fpsLimit": 120,
  "particles": {
    "rotate": {
      "value": {
        "min": 0,
        "max": 360
      },
      "animation": {
        "enable": true,
        "speed": 60
      },
      "direction": "random"
    },
    "color": {
      "value": [
        "#ff1ce7",
        "#1cddff"
      ]
    },
    "move": {
      "distance": {
        "horizontal": 75,
        "vertical": 75
      },
      "direction": "none",
      "enable": true,
      "outModes": {
        "default": "none"
      },
      "random": true,
      "size": false,
      "speed": {
        "min": 1,
        "max": 1
      }
    },
    "number": {
      "limit": {
        "mode": "delete",
        "value": 0
      },
      "value": 0.3
    },
    "opacity": {
      "value": {
        "min": 0.4,
        "max": 1
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 150,
        "decay": 1,
        "delay": 0,
        "mode": "random",
        "startValue": "random",
        "destroy": "none"
      }
    },
    "shape": {
      "close": true,
      "fill": true,
      "type": "triangle"
    },
    "size": {
      "value": {
        "min": 1.2,
        "max": 2
      },
      "animation": {
        "count": 0,
        "enable": true,
        "speed": 13,
        "decay": 0,
        "delay": 0,
        "sync": false,
        "mode": "random",
        "startValue": "max",
        "destroy": "none"
      }
    },
    "stroke": {
      "width": 1
    },
    "links": {
      "blink": false,
      "color": "random",
      "consent": false,
      "distance": 2,
      "enable": true,
      "frequency": 1,
      "opacity": 0.2,
      "shadow": {
        "blur": 1,
        "color": {
          "value": "#ff9900"
        },
        "enable": false
      },
      "triangles": {
        "enable": true,
        "frequency": 1
      },
      "width": 1
    }
  },
  "zLayers": 100,
  "name": "Global",
  "interactivity": {
    "detectsOn": "window",
    "events": {
      "onClick": {
        "enable": true,
        "mode": "trail"
      },
      "onHover": {
        "enable": true,
        "mode": [
          "bubble",
          "connect",
          "grab"
        ],
        "parallax": {
          "enable": true,
          "force": 150,
          "smooth": 4
        }
      }
    },
    "modes": {
      "grab": {
        "distance": 1,
        "links": {
          "opacity": 0.8
        }
      },
      "connect": {
        "radius": 2,
        "distance": 2,
        "links": {
          "opacity": 0.1,
          "width": 0.1
        }
      },
      "trail": {
        "delay": 0.075,
        "pauseOnStop": true,
        "quantity": 1
      },
      "attract": {
        "distance": 2,
        "duration": 0,
        "easing": "ease-out-quad",
        "factor": 0.05,
        "maxSpeed": 70,
        "speed": 1
      },
      "bubble": {
        "distance": 2,
        "duration": 0,
        "mix": true,
        "opacity": 1,
        "size": 0.3
      }
    }
  }
}
